package de.fzi.cep.sepa.flink.test.project;

import de.fzi.cep.sepa.desc.EmbeddedModelSubmitter;
import de.fzi.cep.sepa.desc.declarer.SemanticEventConsumerDeclarer;
import de.fzi.cep.sepa.desc.declarer.SemanticEventProcessingAgentDeclarer;
import de.fzi.cep.sepa.desc.declarer.SemanticEventProducerDeclarer;
import java.lang.Override;
import java.util.ArrayList;
import java.util.List;

public class Init extends EmbeddedModelSubmitter {
  @Override
  protected List<SemanticEventProcessingAgentDeclarer> epaDeclarers() {
    List<SemanticEventProcessingAgentDeclarer> result = new ArrayList<SemanticEventProcessingAgentDeclarer>();
    result.add(new TestProjectController());
    return result;
  }

  @Override
  protected List<SemanticEventProducerDeclarer> sourceDeclarers() {
    List<SemanticEventProducerDeclarer> result = new ArrayList<SemanticEventProducerDeclarer>();
    return result;
  }

  @Override
  protected List<SemanticEventConsumerDeclarer> consumerDeclarers() {
    List<SemanticEventConsumerDeclarer> result = new ArrayList<SemanticEventConsumerDeclarer>();
    return result;
  }

  @Override
  protected int port() {
    return 8080;
  }

  @Override
  protected String contextPath() {
    return "/testproject";
  }
}