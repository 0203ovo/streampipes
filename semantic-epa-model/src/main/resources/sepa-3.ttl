# baseURI: http://event-processing.org/sepa
# imports: http://purl.oclc.org/NET/muo/muo#
# imports: http://spinrdf.org/spin
# imports: http://topbraid.org/schema/schema.ttl
# imports: http://topbraid.org/spin/spinmapl

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sepa: <http://event-processing.org/sepa#> .
@prefix sepa-3: <http://event-processing.org/sepa-3#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix spl: <http://spinrdf.org/spl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://event-processing.org/sepa>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.oclc.org/NET/muo/muo#> ;
  owl:imports <http://spinrdf.org/spin> ;
  owl:imports <http://topbraid.org/schema/schema.ttl> ;
  owl:imports <http://topbraid.org/spin/spinmapl> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
sepa:EventProperty
  rdf:type rdfs:Class ;
.
sepa:EventSchema
  rdf:type rdfs:Class ;
.
sepa:EventStream
  rdf:type rdfs:Class ;
.
sepa:SemanticEventProducer
  rdf:type rdfs:Class ;
.
sepa:hasEventProperty
  rdf:type owl:ObjectProperty ;
  rdfs:domain sepa:EventSchema ;
  rdfs:range sepa:EventProperty ;
.
sepa:hasMeasurementUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain sepa:EventProperty ;
  rdfs:range <http://purl.oclc.org/NET/muo/muo#UnitOfMeasurement> ;
.
sepa:hasPropertyName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain sepa:EventProperty ;
  rdfs:range xsd:string ;
.
sepa:hasSchema
  rdf:type owl:ObjectProperty ;
  rdfs:domain sepa:EventStream ;
  rdfs:range sepa:EventSchema ;
.
sepa:hasType
  rdf:type owl:ObjectProperty ;
  rdfs:domain sepa:EventProperty ;
  rdfs:range rdfs:Resource ;
.
sepa:producesStream
  rdf:type owl:ObjectProperty ;
  rdfs:domain sepa:SemanticEventProducer ;
  rdfs:range sepa:EventStream ;
.
