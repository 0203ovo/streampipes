<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
        <artifactId>semantic-epa-parent</artifactId>
        <groupId>de.fzi.cep.sepa</groupId>
        <version>0.40.2-SNAPSHOT</version>
    </parent>
	<artifactId>semantic-epa-sources-mhwirth</artifactId>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<groupId>de.fzi.cep.sepa</groupId>
			<artifactId>semantic-epa-sources</artifactId>
		</dependency>
		<dependency>
			<groupId>de.fzi.cep.sepa</groupId>
			<artifactId>semantic-epa-client-container</artifactId>
		</dependency>
	</dependencies>
	<repositories>
	<repository>
			<id>laus</id>
			<name>nexus repository</name>
			<url>http://laus.fzi.de/nexus/content/repositories/public/</url>
			<releases>
				<updatePolicy>daily</updatePolicy>
			</releases>
			<snapshots>
			</snapshots>
		</repository>
	</repositories>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.1</version>
					<configuration>
						<source>1.8</source>
						<target>1.8</target>
						<encoding>UTF-8</encoding>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
		<finalName>sources-mhwirth</finalName>
	</build>
</project>
