      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
  </fileSets>
  <requiredProperties>
    <requiredProperty key="classNamePrefix"></requiredProperty>
    <requiredProperty key="elementName"></requiredProperty>
  </requiredProperties>
</archetype-descriptor>